  <% if @range =='User' %>
      <table>
        <tr>
          <th></th>
          <th>ユーザー</th>
          <th></th>
        </tr>
        <% @users.each do |user| %>
          <tr>
            <td><%= image_tag user.get_profile_image, size: '40x40' %></td>
            <td>
              <%= link_to user_path(user) do %>
                <%= user.name %>
              <% end %>
            </td>
            <td></td>
          </tr>
        <% end %>
      </table>
  <% else %>
    <table>
      <tr>
        <th>タイトル</th>
        <th></th>
        <th></th>
      </tr>
      <% @books.each do |book| %>
        <tr>
          <td>
            <%= link_to book_path(book) do %>
              <%= book.title %>
            <% end %>
          </td>
          <td></td>
        </tr>
      <% end %>
    </table>
  <% end %>